    <script>
        if ($('#datatable') != null) {
            $('#datatable').DataTable({
                "language": {
                    "url": "//cdn.datatables.net/plug-ins/9dcbecd42ad/i18n/Turkish.json"
                }
            });
        }
        function success() {
            $("#mainnav-horizontal-inner").append("<div class='alert alert-success alert-dismissable'><strong>İşlem başarılı.</strong></div>");
            setTimeout(function () {
                $("div.alert").fadeOut("slow", function () {
                    $("div.alert").remove();
                });
            }, 2500);
        }
        function error() {
            $("#mainnav-horizontal-inner").append("<div class='alert alert-danger alert-dismissable'><strong>İşlem başarısız.</strong></div>");
            setTimeout(function () {
                $("div.alert").fadeOut("slow", function () {
                    $("div.alert").remove();
                });
            }, 2500);
        }
        function warning() {
            $("#mainnav-horizontal-inner").append("<div class='alert alert-warning'><strong>İşlemi kontrol ediniz.</strong></div>");
            setTimeout(function () {
                $("div.alert").fadeOut("slow", function () {
                    $("div.alert").remove();
                });
            }, 2500);
        }
        function successMessage(val) {
            $("#mainnav-horizontal-inner").append("<div class='alert alert-success alert-dismissable'><strong>" + val + "</strong></div>");
            setTimeout(function () {
                $("div.alert").fadeOut("slow", function () {
                    $("div.alert").remove();
                });
            }, 2500);
        }
        function errorMessage(val) {
            $("#mainnav-horizontal-inner").append("<div class='alert alert-danger alert-dismissable'><strong>" + val + "</strong></div>");
            setTimeout(function () {
                $("div.alert").fadeOut("slow", function () {
                    $("div.alert").remove();
                });
            }, 2500);
        }
        function warningMessage(val) {
            $("#mainnav-horizontal-inner").append("<div class='alert alert-warning alert-dismissable'><strong>" + val + "</strong></div>");
            setTimeout(function () {
                $("div.alert").fadeOut("slow", function () {
                    $("div.alert").remove();
                });
            }, 2500);
        }
        $('.file-upload-button').click(function () {
            $.ajax({
                url: "/File/FileUploadActivePasive",
                method:"POST",                
                success: function (data) {
                    successMessage(data);
                },
                error: function (xhr, status, error) {
                    console.log("Error: file-upload-button ajax");
                    error();
                }
            });
        });
    </script>
    @if (ViewBag.JsFunc != null)
    {
